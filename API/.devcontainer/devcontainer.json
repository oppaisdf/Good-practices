{
  "name": "api-dotnet8",
  "image": "mcr.microsoft.com/devcontainers/dotnet:8.0",
  "customizations": {
    "vscode": {
      "extensions": ["ms-dotnettools.csdevkit"]
    }
  },
  "forwardPorts": [5000, 5001],
  "containerEnv": {
    "ASPNETCORE_URLS": "http://+:5000;https://+:5001",
    "DOTNET_ENVIRONMENT": "Development"
  }
}
